{
  "name": "svgvisualizer",
  "displayName": "svgvisualizer",
  "description": "Visualize your svg icons inside vscode ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:xml",
    "onLanguage:svg"
  ],
  "main": "./extension.js",
  "contributes": {
    "hover": [
      {
        "language": "svg",
        "selector": "*.svg",
        "provider": "./hoverProvider.js"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
